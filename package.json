{
  "name": "pcf-react",
  "version": "1.0.0",
  "description": "A PowerApps Component Framework React starter kit",
  "scripts": {
    "storybook": "start-storybook",
    "lint": "eslint --ext .jsx,.js src/",
    "test": "npx jest --verbose ./test",    
    "//NPM": "*****************************************************************",
    "buildDev": "npx eslint src && npx webpack --config webpack/config.dev.js",
    "buildDevChunk": "npx eslint src && npx webpack --config webpack/config.dev.chunk.js",
    "buildDevForm": "npx eslint src && npx webpack --config webpack/config.dev.form.js",
    "buildPrd": "npx webpack --config webpack/config.prd.js",
    "buildPrdChunk": "npx webpack --config webpack/config.prd.chunk.js",
    "buildPrdForm": "npx webpack --config webpack/config.prd.form.js",    
    "//PCF": "*****************************************************************",
    "build": "pcf-scripts build",
    "clean": "pcf-scripts clean",
    "rebuild": "pcf-scripts rebuild",
    "start": "pcf-scripts start"
  },
  "keywords": [
    "pcf",
    "react",
    "PowerApps Control Framework"
  ],
  "author": "Rex Kenley Tan",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.2.1",
    "@types/jest": "^24.0.25",
    "@types/node": "^10.12.18",
    "@types/powerapps-component-framework": "^1.2.0",
    "@uifabric/icons": "^7.3.0",
    "office-ui-fabric-react": "^7.78.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@storybook/react": "^5.2.8",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-storyshots": "^5.2.8",    
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-require-context-hook": "^1.0.0",    
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.3",
    "pcf-scripts": "^1",
    "pcf-start": "^1",
    "prettier": "1.19.1",
    "react-test-renderer": "^16.12.0"
  }
}
